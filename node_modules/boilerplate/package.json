{
  "name": "boilerplate",
  "description": "Tools and conventions for authoring and using declarative configurations for project \"boilerplates\" that can be consumed by any build system or project scaffolding tool.",
  "version": "0.6.1",
  "homepage": "https://github.com/jonschlinkert/boilerplate",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/boilerplate",
  "bugs": {
    "url": "https://github.com/jonschlinkert/boilerplate/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base": "^0.11.1",
    "base-plugins": "^0.4.13",
    "define-property": "^0.2.5",
    "expand-target": "^0.6.4",
    "expand-utils": "^0.2.2",
    "is-scaffold": "^0.1.1",
    "isobject": "^2.1.0",
    "lazy-cache": "^2.0.1",
    "mixin-deep": "^1.1.3",
    "scaffold": "^0.2.13"
  },
  "devDependencies": {
    "assemble": "^0.16.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.5.3",
    "should": "^9.0.2"
  },
  "keywords": [
    "app",
    "boilerplate",
    "boilerplates",
    "bootstrap",
    "build",
    "cli",
    "dev",
    "development",
    "framework",
    "generate",
    "generator",
    "grunt",
    "gulpfriendly",
    "plugin",
    "project",
    "scaffold",
    "scaffolding",
    "skeleton",
    "start",
    "starter",
    "system",
    "template",
    "tool",
    "toolkit",
    "yeoman"
  ],
  "lint-deps": {
    "ignore": [
      "recipes"
    ]
  },
  "verb": {
    "toc": true,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "boilerplate",
        "generate",
        "scaffold",
        "templates",
        "update",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "broccoli",
      "grunt",
      "gulp",
      "metalsmith",
      "verb",
      "verb-generate-readme",
      "yeoman"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
