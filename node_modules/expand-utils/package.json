{
  "name": "expand-utils",
  "description": "Utils shared by the expand libs.",
  "version": "0.2.2",
  "homepage": "https://github.com/jonschlinkert/expand-utils",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/expand-utils",
  "bugs": {
    "url": "https://github.com/jonschlinkert/expand-utils/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "define-property": "^0.2.5",
    "isobject": "^2.1.0",
    "lazy-cache": "^2.0.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "use": "^2.0.0"
  },
  "keywords": [
    "expand",
    "util"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "expand-config",
        "expand-files",
        "expand-target",
        "expand-task",
        "normalize-config"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "boilerplate",
      "scaffold",
      "verb",
      "verb-readme-generator",
      "expand-task",
      "grunt",
      "gulp"
    ]
  }
}
